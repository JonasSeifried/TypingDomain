{
  "name": "@typing-domain/client",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": "^20.10.0"
  },
  "scripts": {
    "pre": "npm run gen:version",
    "dev": "npm run pre && vite",
    "build": "npm run pre && vue-tsc --noEmit && vite build",
    "start": "vite preview",
    "prod": "npm run build && npm run start",
    "lint": "eslint ./src --ext .ts,.vue",
    "format": "prettier --write src/",
    "gen:version": "node scripts/version.js"
  },
  "dependencies": {
    "firebase": "^10.11.0",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.1",
    "shared": "workspace:*",
    "socket.io-client": "^4.7.5",
    "vue": "^3.4.25",
    "vue-router": "^4.3.2"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^20.12.7",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.19",
    "npm-run-all2": "^6.1.2",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.3",
    "typescript": "~5.4.5",
    "vite": "^5.2.10",
    "vue-tsc": "^2.0.14"
  }
}
